tring,\n      baseCost: Number\n    },\n    computed: {\n      priceClass() {\n        return cookies >= this.baseCost ? 'price-text green' : 'price-text red';\n      }\n    },\n    methods: {\n      doBuy(event) {\n        buy(event);\n      }\n    }\n  }\n  </script>\n  \n  <style scoped>\n  .item {\n    position: relative;\n    width: 100%;\n    height: 64px;\n    border: 6px solid #00000019;\n    user-select: none;\n  }\n  \n  .item:hover {\n    cursor: pointer;\n    -webkit-filter: brightness(115%);\n  }\n  \n  .thumbnail {\n    position: absolute;\n    top: -4px;\n    left: 0;\n    width: 64px;\n    height: 64px;\n    filter: drop-shadow(0 0 2px black);\n  }\n  \n  .name {\n    position: absolute;\n    top: 0;\n    left: 64px;\n    font-size: 1.6em;\n    font-weight: bold;\n    text-shadow: 0 1px 4px black;\n  }\n  \n  .price {\n    position: absolute;\n    bottom: 0;\n    left: 64px;\n  }\n  \n  .small-cookie {\n    width: 1em;\n    height: 1em;\n    filter: drop-shadow(0 0 1px black);\n  }\n  \n  .price-text {\n    font-size: 0.8em;\n    text-shadow: 0 1px 2px black;\n  }\n  \n  .green {\n    color: green;\n  }\n  \n  .red {\n    color: red;\n  }\n  </style>\n  "]},"metadata":{},"sourceType":"module","externalDependencies":[]}