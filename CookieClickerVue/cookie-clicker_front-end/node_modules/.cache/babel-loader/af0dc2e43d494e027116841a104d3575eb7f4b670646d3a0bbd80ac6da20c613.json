{"ast":null,"code":"import { cookies } from '../../public/js/index/cookieHandler.js';\nimport { buy, cursors, grandmas, farms, mines, factories, banks, wizards, shipments, alchemies, portals, timeMs, antis, prisms, chances, fractals, javas, idles, cortexs } from '../../public/js/index/cpsUpgradeHandler.js';\nexport default {\n  name: 'StoreBuilding',\n  props: {\n    name: String,\n    description: String,\n    identifier: String,\n    imgName: String,\n    baseCost: Number\n  },\n  data() {\n    return {\n      currentCookies: cookies\n    };\n  },\n  computed: {\n    priceClass() {\n      return this.currentCookies >= this.baseCost ? 'price-text green' : 'price-text red';\n      return this.b;\n    }\n  },\n  mounted() {\n    setInterval(() => {\n      this.currentCookies = cookies;\n    }, 1); // update every half second\n  },\n\n  methods: {\n    doBuy: function (event) {\n      buy(event);\n    }\n  }\n};","map":{"version":3,"mappings":"AAaA,SAASA,OAAM,QAAS,wCAAuC;AAC/D,SAAOC,GAAG,EAAEC,OAAO,EAAEC,QAAQ,EAAEC,KAAK,EAAEC,KAAK,EAAEC,SAAS,EAAEC,KAAK,EAAEC,OAAO,EAAEC,SAAS,EAAEC,SAAS,EAAEC,OAAO,EAAEC,MAAM,EAAEC,KAAK,EAAEC,MAAM,EAAEC,OAAO,EAAEC,QAAQ,EAAEC,KAAK,EAAEC,KAAK,EAAEC,OAAO,QAAO,4CAA2C;AAExN,eAAc;EACVC,IAAI,EAAC,eAAe;EACpBC,KAAK,EAAC;IACFD,IAAI,EAACE,MAAM;IACXC,WAAW,EAACD,MAAM;IAClBE,UAAU,EAACF,MAAM;IACjBG,OAAO,EAACH,MAAM;IACdI,QAAQ,EAACC;EACb,CAAC;EACDC,IAAI,GAAG;IACH,OAAO;MACPC,cAAc,EAAE7B;IAChB;EACJ,CAAC;EACD8B,QAAQ,EAAE;IACNC,UAAU,GAAG;MACT,OAAO,IAAI,CAACF,cAAa,IAAK,IAAI,CAACH,QAAO,GAAI,kBAAiB,GAAI,gBAAgB;MACnF,OAAO,IAAI,CAACM;IAChB;EACJ,CAAC;EACDC,OAAO,GAAG;IACNC,WAAW,CAAC,MAAM;MACd,IAAI,CAACL,cAAa,GAAI7B,OAAO;IACjC,CAAC,EAAE,CAAC,CAAC,EAAE;EACX,CAAC;;EACDmC,OAAO,EACP;IACIC,KAAK,EAAC,UAASC,KAAK,EACpB;MACIpC,GAAG,CAACoC,KAAK,CAAC;IACd;EACJ;AACJ","names":["cookies","buy","cursors","grandmas","farms","mines","factories","banks","wizards","shipments","alchemies","portals","timeMs","antis","prisms","chances","fractals","javas","idles","cortexs","name","props","String","description","identifier","imgName","baseCost","Number","data","currentCookies","computed","priceClass","b","mounted","setInterval","methods","doBuy","event"],"sourceRoot":"","sources":["C:\\Users\\lakho\\OneDrive - southhills.edu\\Documents\\IT211\\CookieClickerVue\\CookieClickerVue\\cookie-clicker_front-end\\src\\components\\Building.vue"],"sourcesContent":["<template>\n    <div @click=\"doBuy($event)\" class=\"item\" :id=\"identifier\" style=\"background-image: url('/images/storeTile.jpg'); width: 305px; \">\n        <img class=\"thumbnail\" :src=\"'/images/'+imgName\" draggable=\"false\"/>\n        <div class=\"name\">{{ name }}</div>\n        <div class=\"price\">\n            <img class=\"small-cookie\" src=\"/images/cookie.png\" draggable=\"false\"/>\n            <span :class=\"priceClass\" id=\"price-text\">{{ baseCost }}</span>\n        </div>\n        \n    </div>\n</template>\n\n<script>\nimport { cookies } from '../../public/js/index/cookieHandler.js'\nimport{buy, cursors, grandmas, farms, mines, factories, banks, wizards, shipments, alchemies, portals, timeMs, antis, prisms, chances, fractals, javas, idles, cortexs} from '../../public/js/index/cpsUpgradeHandler.js'\n\nexport default{\n    name:'StoreBuilding',\n    props:{\n        name:String,\n        description:String,\n        identifier:String,\n        imgName:String,\n        baseCost:Number\n    },\n    data() {\n        return {\n        currentCookies: cookies\n        }\n    },\n    computed: {\n        priceClass() {\n            return this.currentCookies >= this.baseCost ? 'price-text green' : 'price-text red';\n            return this.b\n        },\n    },\n    mounted() {\n        setInterval(() => {\n            this.currentCookies = cookies;\n        }, 1); // update every half second\n    },\n    methods:\n    {\n        doBuy:function(event)\n        {\n            buy(event);\n        },\n    },\n}\n\n</script>\n\n<style scoped>\n.item\n{\n    position: relative;\n    width: 100%;\n    height: 64px;\n    border: 6px solid #00000019;\n    user-select: none;/* not able to drag image*/\n}\n\n.item:hover\n{\n    cursor: pointer;\n    -webkit-filter: brightness(115%);\n}\n\n.img:hover{\n    box-shadow: 0px 0px 16px #fed inset, 0px 0px 1px #000;\n}\n\n.thumbnail\n{\n    position: absolute;\n    top:-4px;\n    left:0;\n    width:64px;\n    height:64px;\n    filter:drop-shadow(0 0 2px black);\n}\n\n.name\n{\n    position: absolute;\n    top:0;\n    left:64px;\n    font-size: 1.6em;\n    font-weight: bold;\n    text-shadow: 0 1px 4px black;\n}\n.price\n{\n    position:absolute;\n    bottom:0;\n    left:64px;\n}\n.small-cookie\n{\n    width:1em;/* as wide/tall as a text element(font)*/\n    height:1em;\n    filter:drop-shadow(0 0 1px black);\n}\n#price-text\n{\n    font-size: 0.8em;\n    text-shadow: 0 1px 2px black;\n}\n.green{\n    color:green;\n}\n.red{\n    color:red;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}